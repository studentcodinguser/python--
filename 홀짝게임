#홀짝게임

import random

com = random.randint(1, 2)
user = int(input("odd : 1 : even : 2\n"))

if com == 1:
    print('COM(odd)', end=' : ')
else :
    print('COM(even)', end=' : ')

if user == 1:
    print('USER(odd)')
else :
    print('USER(even)')

if com == user :
    print("Correct")
else :
    print("InCorrect")
